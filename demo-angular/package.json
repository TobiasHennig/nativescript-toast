{
  "description": "NativeScript Application",
  "license": "SEE LICENSE IN <your-license-filename>",
  "readme": "NativeScript Application",
  "repository": "<fill-your-repository-here>",
  "scripts": {
    "preclean": "npm i del-cli",
    "clean": "del -f platforms target && npm install",
    "postclean": "git checkout package.json && git checkout tsconfig.json",
    "tsc": "tsc",
    "ns-bundle": "ns-bundle",
    "build.plugin": "cd ../src && npm run build",
    "ci.tslint": "npm i && tslint --config '../tslint.json' 'app/**/*.ts' --exclude '**/node_modules/**'",
    "ci.android.build": "npm run build.plugin && tns prepare android && tns build android",
    "ci.ios.build": "SWIFT_VERSION=4 npm run build.plugin && tns prepare ios && tns build ios",
    "publish-ios-bundle": "SWIFT_VERSION=4 tns build ios --bundle --publish-app",
    "generate-android-snapshot": "generate-android-snapshot --targetArchs arm,arm64,ia32 --install",
    "start-android-bundle": "tns build android --bundle --run-app",
    "start-ios-bundle": "SWIFT_VERSION=4 tns build ios --bundle --run-app",
    "build-android-bundle": "tns build android  --env.snapshot --env.uglify --env.aot",
    "build-ios-bundle": "tns build ios --env.uglify --env.aot",
    "e2e": "tsc -p e2e && mocha --opts ./e2e/config/mocha.opts",
    "e2e-watch": "tsc -p e2e --watch"
  },
  "nativescript": {
    "id": "org.nativescript.toastdemoapp",
    "tns-android": {
      "version": "5.1.0"
    },
    "tns-ios": {
      "version": "5.1.1"
    }
  },
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "del-cli": "^1.1.0",
    "nativescript-angular": "^7.2.0",
    "nativescript-theme-core": "~1.0.4",
    "nativescript-toast": "*",
    "nativescript-ui-core": "*",
    "nativescript-ui-gauge": "*",
    "reflect-metadata": "~0.1.10",
    "rxjs": "^6.3.3",
    "tns-core-modules": "^5.1.2",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/compiler": "~7.2.0",
    "@angular/compiler-cli": "~7.2.0",
    "@ngtools/webpack": "~7.2.0",
    "@types/chai": "~4.1.3",
    "@types/mocha": "~5.2.1",
    "@types/node": "^7.0.5",
    "babel-traverse": "6.12.0",
    "babel-types": "6.11.1",
    "babylon": "6.8.4",
    "filewalker": "0.1.2",
    "karma": "^3.0.0",
    "karma-nativescript-launcher": "^0.4.0",
    "lazy": "1.0.11",
    "nativescript-css-loader": "~0.26.1",
    "nativescript-dev-appium": "^4.0.9",
    "nativescript-dev-typescript": "^0.7.9",
    "nativescript-dev-webpack": "^0.19.1",
    "tslint": "~5.11.0",
    "typescript": "3.1.1"
  }
}
